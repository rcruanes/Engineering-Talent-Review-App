-- -----------------------------------------------------
-- Table `wdc_prototype_recommender_db_two`.`users`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `wdc_prototype_recommender_db_two`.`users` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `first_name` VARCHAR(45) NULL DEFAULT NULL,
  `last_name` VARCHAR(45) NULL DEFAULT NULL,
  `email` VARCHAR(45) NULL DEFAULT NULL,
  `pw` CHAR(60) NULL DEFAULT NULL,
  `role` ENUM('Nominator', 'Recommender', 'Review Committee') NULL DEFAULT NULL,
  `created_at` DATETIME NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` DATETIME NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`))
ENGINE = InnoDB
AUTO_INCREMENT = 16
DEFAULT CHARACTER SET = utf8mb3;


-- -----------------------------------------------------
-- Table `wdc_prototype_recommender_db_two`.`nominees`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `wdc_prototype_recommender_db_two`.`nominees` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `user_id` INT NOT NULL,
  `first_name` VARCHAR(45) NULL DEFAULT NULL,
  `last_name` VARCHAR(45) NULL DEFAULT NULL,
  `department_name` VARCHAR(45) NULL DEFAULT NULL,
  `job_category` VARCHAR(45) NULL DEFAULT NULL,
  `email` VARCHAR(45) NULL DEFAULT NULL,
  `nominator_qualification` TEXT NULL DEFAULT NULL,
  `created_at` DATETIME NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` DATETIME NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`),
  INDEX `fk_nominees_users1_idx` (`user_id` ASC) VISIBLE,
  CONSTRAINT `fk_nominees_users1`
    FOREIGN KEY (`user_id`)
    REFERENCES `wdc_prototype_recommender_db_two`.`users` (`id`))
ENGINE = InnoDB
AUTO_INCREMENT = 18
DEFAULT CHARACTER SET = utf8mb3;


-- -----------------------------------------------------
-- Table `wdc_prototype_recommender_db_two`.`activities`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `wdc_prototype_recommender_db_two`.`activities` (
