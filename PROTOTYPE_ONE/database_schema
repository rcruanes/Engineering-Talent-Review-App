  PRIMARY KEY (`id`),
  INDEX `fk_recommender_forms_users1_idx` (`user_id` ASC) VISIBLE,
  INDEX `fk_recommender_forms_nominees1_idx` (`nominee_id` ASC) VISIBLE,
  CONSTRAINT `fk_recommender_forms_nominees1`
    FOREIGN KEY (`nominee_id`)
